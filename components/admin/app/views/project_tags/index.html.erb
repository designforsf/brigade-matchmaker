<p id="notice"><%= notice %></p>

<h1>Project Tags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_tags.each do |project_tag| %>
      <tr>
        <td><%= link_to 'Destroy', project_project_tag_path(@project, project_tag), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
